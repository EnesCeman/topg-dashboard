{
    "buildCommand": "npm i && cd client && npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/api/server.js"
        }
    ]
}